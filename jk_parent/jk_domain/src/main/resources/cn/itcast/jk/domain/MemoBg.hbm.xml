<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.jk.domain.MemoBg" table="MEMO_PIC_BG" dynamic-insert="true" dynamic-update="true">
		<id name="id" column="MEMO_PIC_BG_ID">
			<!--hibernate主键生成策略：native, increment,sequence,uuid,assigned,identity  -->
			<generator class="uuid"/>
		</id>
		<property name="headpath" column="HEAD_PATH"/>
		<property name="bodypath" column="BODY_PATH"/>
		<property name="tailpath" column="TAIL_PATH"/>
		
	</class>
</hibernate-mapping>