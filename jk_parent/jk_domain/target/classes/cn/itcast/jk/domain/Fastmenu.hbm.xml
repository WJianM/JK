<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.jk.domain">
	<!-- 配置类的映射 -->
	<class name="Fastmenu" table="FASTMENU">
		<!-- 1.配置主键生成策略 -->
		<id name="id" column="ID">
			<generator class="uuid"/>
		</id>
		<!-- 2.配置其他属性 -->
		<property name="mname" column="MNAME" type="string"/>
		<property name="murl" column="MURL" type="string"/>
		<property name="count" column="COUNT" type="double"/>
		<property name="userid" column="USERID" type="string"/>

	</class>

</hibernate-mapping>